    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "main": "main.js",
  "build": {
    "appId": "memorise.panel.7",
    "files": [
      "out",
      "main.js"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
