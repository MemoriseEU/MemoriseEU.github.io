{
  "name": "memorise-panel-7",
  "version": "1.0.0",
  "private": true,
  "description": "The memorise panel 7 for the exhibition in Bergen-Belsen",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
